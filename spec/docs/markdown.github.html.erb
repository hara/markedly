<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="<%= css %>">
    <script>
      var ws = new WebSocket('ws://localhost:8080');

      ws.onmessage = function(event) {
        if (event.data == 'updated') {
          window.location.reload();
        }
      };

      window.onunload = function() {
        ws.close();
      };
    </script>
  </head>
  <body>
    <div class="container">
      <h1>Markedly</h1>

    </div>
  </body>
</html>
